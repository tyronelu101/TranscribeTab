<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/tabListFragment">
    <fragment
        android:id="@+id/tabListFragment"
        android:name="com.simplu.transcribetab.tablist.TabListFragment"
        android:label="fragment_tab_list"
        tools:layout="@layout/fragment_tab_list" >
        <action
            android:id="@+id/action_tabListFragment_to_songListFragment"
            app:destination="@id/songListFragment" />
        <action
            android:id="@+id/action_tabListFragment_to_tabParentFragment"
            app:destination="@id/tabParentFragment" />
    </fragment>
    <fragment
        android:id="@+id/songListFragment"
        android:name="com.simplu.transcribetab.SongListFragment"
        android:label="SongListFragment" >
        <action
            android:id="@+id/action_songListFragment_to_editTabParentFragment"
            app:destination="@id/editTabParentFragment"
            app:popUpTo="@id/tabListFragment" />
    </fragment>
    <fragment
        android:id="@+id/editTabParentFragment"
        android:name="com.simplu.transcribetab.edittab.EditTabParentFragment"
        android:label="fragment_edit_tab_parent"
        tools:layout="@layout/fragment_edit_tab_parent" >
        <argument
            android:name="songUri"
            app:argType="string" />
        <argument
            android:name="tab"
            app:argType="com.simplu.transcribetab.database.Tablature"
            app:nullable="true" />
        <action
            android:id="@+id/action_editTabParentFragment_to_tabParentFragment"
            app:destination="@id/tabParentFragment" />
    </fragment>
    <fragment
        android:id="@+id/tabParentFragment"
        android:name="com.simplu.transcribetab.tab.TabParentFragment"
        android:label="fragment_tab_parent"
        tools:layout="@layout/fragment_tab_parent" >
        <action
            android:id="@+id/action_tabParentFragment_to_editTabParentFragment"
            app:destination="@id/editTabParentFragment" />
        <argument
            android:name="tab"
            app:argType="com.simplu.transcribetab.database.Tablature" />
    </fragment>
</navigation>